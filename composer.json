{
  "name": "irmmr/handle",
  "type": "library",
  "description": "A basic library for make coding easy from AMA",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "keywords": [
    "handler",
    "base"
  ],
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "authors": [
    {
      "name": "irmmr",
      "email": "irmmr.ir@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Irmmr\\Handle\\App\\": "app/",
      "Irmmr\\Handle\\": "src/"
    }
  }
}
