{
  "name": "irmmr/handle",
  "type": "project",
  "description": "A basic library for make coding easy from AMA",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "MIT",
  "keywords": [
    "handler",
    "base"
  ],
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "authors": [
    {
      "name": "irmmr",
      "email": "irmmr.ir@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "phpmailer/phpmailer": "^6.3"
  },
  "autoload": {
    "files" : [
      "/config.php"
    ],
    "psr-4": {
      "Irmmr\\Handle\\": "src/",
      "Irmmr\\Handle\\App\\": "app/"
    }
  }
}